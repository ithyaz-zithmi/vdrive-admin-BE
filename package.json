  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:auth": "jest __tests__ --testNamePattern=\"Auth\"",
    "test:locations": "jest __tests__ --testNamePattern=\"Location\"",
    "test:hotspots": "jest __tests__ --testNamePattern=\"Hotspot\"",
    "test:price-settings": "jest __tests__ --testNamePattern=\"Price\"",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "seed": "ts-node src/db/setup.ts",
    "migrate": "node -r ts-node/register ./node_modules/.bin/node-pg-migrate"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/*.d.ts",
      "!src/server.ts",
      "!src/**/*.test.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ]
  },
